{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "highest-available-document-mode": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "button-type": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ]
  }
}